<div class="row">
  <div class="span6 offset3">
    <%= form_for(@project) do |f| %>
      <div>
        <%= f.label :title %>
        <%= f.text_field :title %>
      </div>

      <div>
        <%= f.label :description %>
        <%= f.text_field :description %>
      </div>

      <div>
        <%= f.submit "Save", class: "btn btn-large btn-primary" %>
      </div>    
    <% end %>
  </div>
</div>