<h1>All users</h1>
<div class-"row">
  <div class="span6 offset3">
    <% if current_user.admin? %>
      <%= link_to "New user", '/new_user', class: "btn btn-large btn-primary" %>
	<% end %>
  <% @users.each do |user| %>
	<div>
	  <br><%= link_to user.username, user %>
    </div>
  <% end %>
  </div>
</div>